{
    "name": "atm-code/ar-php-laravel",
    "description": "ar-php-laravel",
    "keywords": [
        "atm-code",
        "ar-php-laravel",
        "ar-php"
    ],
    "homepage": "https://github.com/atm-code/ar-php-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AtmCode\\ArPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AtmCode\\ArPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AtmCode\\ArPhp\\ArPhpServiceProvider"
            ],
            "aliases": {
                "ArPhp": "AtmCode\\ArPhp\\ArPhpFacade"
            }
        }
    }
}
